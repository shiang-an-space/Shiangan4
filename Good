<!DOCTYPE html>
<html>
<head>
    <title>https://maps.app.goo.gl/VwDBuD7Xe9hZxoei7?g_st=com.google.maps.preview.copy</title>
    <link rel="stylesheet" href="https://unpkg.com/leaflet/dist/leaflet.css" />
    <style>
        #map {
            height: 400px;
            width: 100%;        }
    </style>
</head>
<body>
    <h1>https://maps.app.goo.gl/VwDBuD7Xe9hZxoei7?g_st=com.google.maps.preview.copy</h1>
    <div id="map"></div>

    <script src="https://unpkg.com/leaflet/dist/leaflet.js"></script>
    <script>
        var map = L.map('map').setView([25.033964, 121.564472], 15);
        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            maxZoom: 19,
        }).addTo(map);
        L.marker([25.033964, 121.564472]).addTo(map)
            .bindPopup('公司位置')
            .openPopup();
    </script>
</body>
</html>
